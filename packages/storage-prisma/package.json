{
  "name": "@autorix/storage-prisma",
  "version": "0.1.0",
  "description": "Prisma adapter for Autorix policy storage",
  "keywords": [
    "autorix",
    "authorization",
    "prisma",
    "storage",
    "adapter"
  ],
  "license": "MIT",
  "author": "Chechooxd <sergiogalaz60@gmail.com>",
  "homepage": "https://github.com/adinet-cl/autorix",
  "repository": {
    "type": "git",
    "url": "https://github.com/adinet-cl/autorix.git",
    "directory": "packages/storage-prisma"
  },
  "bugs": {
    "url": "https://github.com/adinet-cl/autorix/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "License"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest"
  },
  "dependencies": {
    "@autorix/core": "workspace:*",
    "@autorix/storage": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@autorix/storage": "^0.1.0",
    "@prisma/client": "^5.0.0 || ^6.0.0"
  }
}
