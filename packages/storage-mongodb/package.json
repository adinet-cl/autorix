{
  "name": "@autorix/storage-mongodb",
  "version": "0.1.0",
  "description": "MongoDB adapter for Autorix policy storage",
  "keywords": [
    "autorix",
    "authorization",
    "mongodb",
    "storage",
    "adapter"
  ],
  "author": "Adinet",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "License"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest"
  },
  "dependencies": {
    "@autorix/core": "workspace:*",
    "@autorix/storage": "workspace:*",
    "mongodb": "^6.12.0"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@autorix/storage": "^0.1.0"
  }
}
