{
  "name": "@autorix/nestjs",
  "version": "0.1.0",
  "description": "NestJS integration for Autorix policy evaluator",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest -c ../../vitest.config.ts run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@autorix/core": "workspace:*",
    "@autorix/storage": "workspace:*",
    "reflect-metadata": "^0.2.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0"
  }
}