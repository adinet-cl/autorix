{
  "name": "@autorix/storage",
  "version": "0.1.0",
  "description": "Storage adapters for Autorix policy evaluator",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest -c ../../vitest.config.ts run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@autorix/core": "workspace:*"
  }
}